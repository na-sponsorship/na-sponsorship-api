        "Male",
        "Female"
      ],
      "required": true
    },
    "Story": {
      "default": "",
      "type": "text",
      "required": true
    },
    "isSponsored": {
      "default": false,
      "type": "boolean"
    },
    "sponsorship": {
      "model": "sponsorship",
      "via": "children"
