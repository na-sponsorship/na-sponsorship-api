{
  "connection": "default",
  "collectionName": "sponsorship",
  "info": {
    "name": "sponsorship",
    "description": "Child sponsorships"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "isActive": {
      "default": false,
      "type": "boolean"
    },
    "children": {
      "model": "child",
      "via": "sponsorship"
    },
    "sponsor": {
      "model": "sponsor",
      "via": "sponsorships"
    },
    "amount": {
      "default": 20,
      "type": "decimal"
    },
    "transactions": {
      "collection": "transaction",
      "via": "sponsorship"
    }
  }
}